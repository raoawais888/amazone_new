

<body class="sub_page">


    <%- include("../partials/header.ejs")  %>

     <!-- end header section -->
  </div>


  <section id="breadcrumb">

    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <h2><%= search  %></h2>
        </div>
      </div>
    </div>

  </section>
  <!-- product section -->

  <section class="product_section layout_padding">
    <div class="container">
      <div class="heading_container heading_center">
        <h2>
          Our Products
        </h2>
      </div>
      <div class="row">

        <% product.forEach(data => { %>
          
        

        <!-- loop section start  -->
        <div class="col-sm-6 col-lg-4">
          <div class="box">
            <div class="img-box">
              <img src="uploads/<%= data.image%>" alt="">
              <a href="/product-detail/<%= data._id %>" class="add_cart_btn">
                <span>
                  Add To Cart
                </span>
              </a>
            </div>
            <div class="detail-box">
              <h5>
               <%= data.name %>
              </h5>
              <div class="product_info">
                <h5>
                  <span>$</span>   <%= data.price %>
                </h5>
                <div class="star_container">
                  <i class="fa fa-star" aria-hidden="true"></i>
                  <i class="fa fa-star" aria-hidden="true"></i>
                  <i class="fa fa-star" aria-hidden="true"></i>
                  <i class="fa fa-star" aria-hidden="true"></i>
                  <i class="fa fa-star" aria-hidden="true"></i>
                </div>
              </div>
            </div>
          </div>
        </div>
       

<!-- loop section nd -->

<% }) %>

      </div>
    
    </div>
  </section>

  <!-- end product section -->


  <%- include("../partials/footer.ejs")  %>