

<%- include('../partials/masterHeader.ejs')%>
<%- include('../partials/homeHeader.ejs')%>
    



  <!-- all brands slider show start -->
    <!-- Categories Section Begin -->
    <section class="categories">
      <div class="container">
          <div class="row">
              <div class="categories__slider owl-carousel">

                    <% brands.forEach((brand , i)=>{ %>
                   
                  <div class="col-lg-3 p-2">
                              
                    <div class="brand_card">
                      <img src="uploads/<%= brand.image %>" alt="">
                    </div>
                       
                          
                      
                  </div>
                 
                    
                 <%  }) %>

              </div>
          </div>
      </div>
  </section>
  <!-- Categories Section End -->
  
   <!-- product section start //////////////////// -->
         
   <section class="home_product spad">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="section-title">
            <h2>Featured Product</h2>
        </div>
        </div>

               

        <% product.forEach(data=> { %>
          <div class="col-lg-3 col-md-6 col-sm-6">
              <div class="product__item">
                  <div class="product__item__pic">
                    <img src="/uploads/<%=data.image  %>" alt="">
                  </div>
                  <div class="product__item__text">
                      <h6><a href="#"><%= data.name %></a></h6>
                      <div class="footer_p">

                        <h5>$<%= data.price  %></h5>
                        <a href=""><i class="fa fa-shopping-cart"></i> </a>
                      </div>
                     
                  </div>
              </div>
          </div>
          <% }) %>

      </div>
    </div>
   </section>
   
   <!-- product section end //////////////////// -->

   


  <%- include('../partials/footer.ejs'); %>
  
  